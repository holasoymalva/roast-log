{
  "name": "roast-log",
  "version": "1.0.0",
  "description": "A Node.js library that enhances console.log with humorous comments and reactions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "test-config": "node scripts/test-config.js",
    "example": "npm run build && node example.js",
    "example:ts": "npm run build && ts-node example.ts",
    "example:simple": "npm run build && ts-node example-simple.ts"
  },
  "keywords": [
    "console",
    "logging",
    "humor",
    "debugging",
    "development",
    "roast-log"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
